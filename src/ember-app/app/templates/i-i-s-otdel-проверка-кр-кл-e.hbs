<h3 class="ui header">{{t "forms.i-i-s-otdel-проверка-кр-кл-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-otdel-проверка-кр-кл-e.дата-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.дата
      class=(if (v-get validationModel "дата" "isInvalid") "error")
      data-test-i-i-s-otdel-проверка-кр-кл-e-дата=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "дата" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.доходыВМес
      class=(if (v-get validationModel "доходыВМес" "isInvalid") "error")
      label=(t "forms.i-i-s-otdel-проверка-кр-кл-e.доходыВМес-caption")
      data-test-i-i-s-otdel-проверка-кр-кл-e-доходыВМес=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "доходыВМес" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.суммаЕжПлЗаКр
      class=(if (v-get validationModel "суммаЕжПлЗаКр" "isInvalid") "error")
      label=(t "forms.i-i-s-otdel-проверка-кр-кл-e.суммаЕжПлЗаКр-caption")
      data-test-i-i-s-otdel-проверка-кр-кл-e-суммаЕжПлЗаКр=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "суммаЕжПлЗаКр" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.наличЗалога
      class=(if (v-get validationModel "наличЗалога" "isInvalid") "error")
      label=(t "forms.i-i-s-otdel-проверка-кр-кл-e.наличЗалога-caption")
      data-test-i-i-s-otdel-проверка-кр-кл-e-наличЗалога=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "наличЗалога" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.наличиеПоручит
      class=(if (v-get validationModel "наличиеПоручит" "isInvalid") "error")
      label=(t "forms.i-i-s-otdel-проверка-кр-кл-e.наличиеПоручит-caption")
      data-test-i-i-s-otdel-проверка-кр-кл-e-наличиеПоручит=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "наличиеПоручит" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-otdel-проверка-кр-кл-e.одобрениеКр-caption"}}</label>
    {{flexberry-checkbox
      readonly=readonly
      value=model.одобрениеКр
      class=(if (v-get validationModel "одобрениеКр" "isInvalid") "error")
      data-test-i-i-s-otdel-проверка-кр-кл-e-одобрениеКр=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "одобрениеКр" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-otdel-проверка-кр-кл-e.организация-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.организация
      displayAttributeName="наименование"
      autocomplete=true
      relationName="организация"
      projection="ОрганизацияL"
      title=(t "forms.i-i-s-otdel-проверка-кр-кл-e.организация-caption")
      readonly=readonly
      componentName="организацияLookup"
      class=(if (v-get validationModel "организация" "isInvalid") "error")
      data-test-i-i-s-otdel-проверка-кр-кл-e-организация=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "организация" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-otdel-проверка-кр-кл-e.заклКрДог-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.заклКрДог
      displayAttributeName="цельКр"
      autocomplete=true
      relationName="заклКрДог"
      projection="ЗаклКрДогL"
      title=(t "forms.i-i-s-otdel-проверка-кр-кл-e.заклКрДог-caption")
      readonly=readonly
      componentName="заклКрДогLookup"
      class=(if (v-get validationModel "заклКрДог" "isInvalid") "error")
      data-test-i-i-s-otdel-проверка-кр-кл-e-заклКрДог=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "заклКрДог" "message")}}
  </div>
</form>
